    moveToBall();
    turnLeft();
    moveToBall();
    turnLeft();
    moveToBall();
    turnLeft();
    moveToBall();
    turnLeft();
}

function moveToBall(){
    for(var i = 0; i < 1; i++){
         move();
         move();
         move();
         move();
         move();
         putBall();
    }
}
