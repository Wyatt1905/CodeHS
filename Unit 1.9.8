function start(){
    jumpHurdle();
    spin();
}

function jumpHurdle(){
    for(var i = 0; i < 5; i++){
        move();
        move();
        turnLeft();
        move();
        turnRight();
        move();
        turnRight();
        move();
        turnLeft();
    }
}

function spin(){
    turnRight();
    turnRight();
    turnRight();
    turnRight();
}
