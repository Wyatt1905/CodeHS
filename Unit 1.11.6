function start() {
    // Karel is facing North or East
    if(facingNorth()){
        makeRightSquare();
    }else{
        makeLeftSquare();
    }
    
}


//THINGS KAREL LEARNS
function makeRightSquare(){
    for(var i = 0; i <4; i++){
        putBall();
        move();
        turnRight();
    }
}



function makeLeftSquare(){
    for(var i = 0; i <4; i++){
        putBall();
        move();
        turnLeft();
    }
}
